#include <stdio.h>
#include <iostream>
using namespace std;

int triangle (int x)
{
for (int i=1; i<=x ;i++)
{
for (int j=1; j<i+1; j++)
	cout <<"T"; cout <<endl;
 }
 for (int i=x-1; i>=1; i--)
 {
 	 for (int j=1; j<i+1; j++)
 	 	 cout <<"T"; cout <<endl;
 }
 return 0;
}


 int main ()
 	 {
 	 int y;
 	 std::cout<<"type the numbers of T you want" <<std::endl;
 	 std::cin >> y;
 	 triangle (y);
 }
